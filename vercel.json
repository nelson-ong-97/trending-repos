{
  "buildCommand": "cd apps/web && bun run build",
  "outputDirectory": "apps/web/.output",
  "installCommand": "bun install",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/sync-repos",
      "schedule": "0 0 * * *"
    }
  ]
}

